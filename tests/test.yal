fun add(a, b) {
	ret a + b;
}

data Square {
	height,
	width
}

// Function to find area of square
fun area(square) {
	ret square.height * square.width;
}

mut state = "1";

fun factorial(n) {
	if (n <= 1) {
		ret 1;
	}

	ret n * factorial(n - 1);
}
